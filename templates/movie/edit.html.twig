{% extends "base.html.twig" %}

{% block title %}

    Movies Page

{% endblock %}

{% block body %}

    <div class="container">
        <div class="row">
            <div class="col-lg-12">
                <h3>Edit Biodata Siswa</h3>
            </div>
        </div>


        <form action="#" enctype="multipart/form-data" method="post">

            <div class="row">
                {{ form_start(form) }}
                <div class="col-md-12 mb-5">
                     {{ form_widget(form) }}
                </div>
                <div class="col-md-12">
                    <a href="{{ path('movie.index') }}" class="btn btn-sm btn-success"> Back</a>
                    <button type="submit" class="btn btn-sm btn-primary">Submit</button>
                </div>
                {{ form_end(form) }}
            </div>
        </form>


    </div>


{% endblock %}
